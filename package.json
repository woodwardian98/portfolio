{
  "name": "visible-venus",
  "version": "1.0.0",
  "description": "A project with a Node.js backend and an Astro frontend.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix astro-nano run dev",
    "build:frontend": "npm --prefix astro-nano run build",
    "start:backend": "npm --prefix backend run start",
    "db:migrate": "npm --prefix backend run db:migrate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/woodwardian98/portfolio.git"
